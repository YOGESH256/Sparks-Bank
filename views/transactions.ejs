<%- include("partials/header") -%>
<div class="container my-2">
        <div class="row">
            <div class="col-lg-8 offset-lg-2">
                <div class="card border-info">
                    <div class="card-header">
                        <h1>All Transactions</h1>
                    </div>
                    <div class="card-body bg-light">
                        <ul class="list-group list-group-flush">
                            <% if(ab.length > 0) { %>
                                <% for(let c of ab) {%>
                                    <li class="list-group-item list-group-item-action list-group-item-light my-2" style="text-transform: capitalize;">
                                        <%= c.payment %> <b>----- <%= c.Date %></b>
                                    </li>
                                    <% } %>
                                        <% } else { %>
                                            <li class="list-group-item" style="text-transform: capitalize;">
                                                Sorry!!!! No transactions Yet........
                                            </li>
                                            <% } %>
                        </ul>
                    </div>
                </div>
            </div>
        </div>
    </div>








<%- include("partials/footer") -%>
